{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Information"
        }
    },
    "ConnectionStrings": {
        "DefaultConnection": "${PGSQL_CONNECTION_STRING}"
    },
    "IdentityServiceUrl": "${IDENTITY_SERVICE_URL}",
    "GitHub": {
        "ValidAudience": "github id required here"
    },
    "RabbitMq": {
        "Host": "${RABBITMQ_HOST}",
        "Username": "${RABBITMQ_USERNAME}",
        "Password": "${RABBITMQ_PASSWORD}"
    },
    "Kestrel": {
        "Endpoints": {
            "Grpc": {
                "Protocols": "${KESTREL_ENDPOINTS_GRPC_PROTOCOLS}",
                "Url": "${KESTREL_ENDPOINTS_GRPC_URL}"
            },
            "WebApi": {
                "Protocols": "${KESTREL_ENDPOINTS_WEBAPI_PROTOCOLS}",
                "Url": "${KESTREL_ENDPOINTS_WEBAPI_URL}"
            }
        }
    }
}
